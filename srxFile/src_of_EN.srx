				<rule break="no">
					<beforebreak>\d+\.\s+</beforebreak>
					<afterbreak>Amtsperiode|Breitengrads?|Breitengrades|Jubiläum|Jhd?|Jhdts?|Konferenz|(Jahres|Partei)(-K|k)onferenz|Längengrade?s?|Tags?|Tages|(Jahres|Spiel|Partei|Geburts)tag|(Jahres|Spiel|Partei|Geburts)tages|(Jahres|Spiel|Partei|Geburts)tags|Jahrhunderts?|Jahrtausend|Platz|Platzes|Lebensjahrs?|Lebensjahres|Lochs?|Loches|Grads|Grades|Obergeschoss|Stock(werk)?s?|Etage|Klasse|Runde|Bezirk|Etappe|Staffel|Sinfonie</afterbreak>
				</rule>

				<!-- English abbreviations - but these work globally for all languages -->
				<rule break="no">
					<beforebreak>\b(Mrs?|No|pp|St|no|Sr|Jr|Bros|[Bb]tw|vs|esp|[Ff]igs?|Jan|Feb|Mar|Apr|Ju[nl]|Aug|Sept?|O[ck]t|Nov|Dec|PhD|BSc|BEng|BComp|BArch|al|cf|Inc|Ms|MEng|MSc|MComp|Gen|Sen|Prof|Corp|Co|co|Ltd)\.\s</beforebreak>
					<afterbreak/>
				</rule>

				<!-- Latin abbreviations - but these work globally for all languages - spp. -->
				<rule break="no">
					<beforebreak>\b(spp?)\.\s</beforebreak>
					<afterbreak/>
				</rule>

				<!-- German abbreviations already merged -->

				<!-- Break rules -->
				<rule break="yes">
					<beforebreak>[\.!?…][\u0002|'|"|“|«|‹|\)|\]|\}¹²³]+\s+</beforebreak>
					<afterbreak/>
				</rule>

				<!-- add \s? to <beforebreak> -->
				<rule break="yes">
					<beforebreak>[\.!?…]['"“\p{Pe}\u00BB\u201D]*\s?</beforebreak>
					<afterbreak>\p{Lu}[^\p{Lu}]</afterbreak>
				</rule>

				<rule break="yes">
					<beforebreak>\s\p{L}[\.!?]\s</beforebreak>
					<afterbreak>\p{Lu}\p{Ll}</afterbreak>
				</rule>

				<!-- z.B. 2 sentences: “Liebst du mich?” “Ja!” -->
				<rule break="yes">
					<beforebreak>[\.!?][”“]</beforebreak>
					<afterbreak>[“„]</afterbreak>
				</rule>


				<rule break="yes">
					<beforebreak>[\.!?…]\s+</beforebreak>
					<afterbreak>\p{Lu}</afterbreak>
				</rule>
			</languagerule>

			<!-- Default language-related segmentation rules -->
			<languagerule languagerulename="Default">
				<!-- Additional rules which share the same component in every languages -->

				<!-- Start sentence with 1a), [a], 1), b), 50), 155) - Break -->
				<rule break="yes">
					<beforebreak>(?m)^\s*(\p{Ps})?[A-Za-z\d]+(\p{Pe}|\.)\s</beforebreak>
					<afterbreak/>
				</rule>

				<!-- DOMINATE RULE -->
				<!-- Start sentence with 1.2. - Break - add \p{Lu}\p{Ll} to <afterbreak> -->
				<rule break="yes">
					<beforebreak>(?m)^(\d+\.\d+\.\d+|\d+\.\d+)[\p{Pe}\.\u003E]?\s</beforebreak>
					<afterbreak>\p{Lu}\p{Ll}</afterbreak>
				</rule>

				<!-- Start sentence with <A.II>, <A.III>, <A.IV> - Break -->
				<rule break="yes">
					<beforebreak>(?m)^\s*\u003C[A-Z]\.[XVI]+\u003E\s</beforebreak>
					<afterbreak/>
				</rule>

				<!-- If FIG. is at the end of the sentence -->
				<rule break="yes">
					<beforebreak>(?i)(figures?|figs?)\.\s\d+\.\s?</beforebreak>
					<afterbreak>\p{Lu}\p{Ll}</afterbreak>
				</rule>

				<!-- PTDR -->

				<!-- Informal sentence? -->
				<!-- . with space and lowercase letter -->
				<rule break="no">
					<beforebreak>\.</beforebreak>
					<afterbreak>\s+\p{Ll}</afterbreak>
				</rule>

				<!-- . with number and ignore character in [] -->
				<rule break="no">
					<beforebreak>\.</beforebreak>
					<afterbreak>\s+\p{N}+[^\.\)\-]</afterbreak>
				</rule>

				<!-- . with number . -->
				<rule break="no">
					<beforebreak>\.\s+\p{N}+\.</beforebreak>
					<afterbreak>\s</afterbreak>
				</rule>

				<!-- ... exception -->
				<rule break="no">
					<beforebreak>\.\.\.</beforebreak>
					<afterbreak>\s+\P{Lu}</afterbreak>
				</rule>

				<!-- numbered headers break rule based on an idea by Jean-Christophe Helary -->
				<!-- SRX file default is not multiline so ^ will not work - add (?m) -->
				<!-- <rule break="yes">
                    <beforebreak>(?m)^\s*\p{Nd}+[\p{Nd}\.\)\]]+</beforebreak>
                    <afterbreak>\s+\p{Lu}</afterbreak>
                </rule> -->

				<!-- .?! break rule -->
				<!-- <rule break="yes">
                    <beforebreak>[\.\?\!]+</beforebreak>
                    <afterbreak>\s</afterbreak>
                </rule> -->

				<!-- language_tools -->

				<!-- PARAGRAPH SEPARATOR Unicode character -->
				<rule break="yes">
					<beforebreak>\u2029</beforebreak>
					<afterbreak/>
				</rule>

				<!-- For smooth interoperability with Anaphraseus translation plugin for OpenOffice.org -->
				<rule break="yes">
					<beforebreak/>
					<afterbreak>&lt;0\}</afterbreak>
				</rule>

				<!-- Anaphraseus segment start marker -->
				<rule break="yes">
					<beforebreak>\{0></beforebreak>
					<afterbreak/>
				</rule>

				<!-- Support srt files (Subtitle) -->
				<rule break="yes">
					<beforebreak>\d{2}:\d{2}:\d{2},\d{3}\r?\n</beforebreak>
					<afterbreak/>
				</rule>

			</languagerule>

			<languagerule languagerulename="GeneralImportant">

				<!-- URLs without "www." -->
				<rule break="no">
					<beforebreak>\b(https?|ftp|file|chrome|chromium|android|(chrome|moz)\-extension):///?[A-Za-z0-9\-]+\.</beforebreak>
					<afterbreak>[A-Za-z0-9\-]+(\.|\b)</afterbreak>
				</rule>

				<!-- Subdomains without "www." (e.g. foo.MyDomain.com) - add .ai and .trade and ? at \. <beforebreak> -->
				<rule break="no">
					<beforebreak>\b[A-Za-z0-9\-]+\.?</beforebreak>
					<afterbreak>[A-Za-z0-9\-]+\.(?i)(ai|trade|com|net|org|info|de|es|edu|co|eu|nl|io|cn|uk|gov|biz|ca|tk|ru|br|jp|pl)(\.|\b)</afterbreak>
				</rule>

				<!-- Don't split up URLs. -->
				<rule break="no">
					<beforebreak>\bwww\.</beforebreak>
					<afterbreak>\w</afterbreak>
				</rule>

				<!-- Don't split up e-mail addresses. -->
				<rule break="no">
					<beforebreak>[\.\!\?]</beforebreak>
					<afterbreak>\S*@</afterbreak>
				</rule>

			</languagerule>

			<!-- Additional rules to handle HTML tag -->
			<languagerule languagerulename="Patent">

				<!-- Additional rules to handle specific cases -->

				<rule break="no">
					<beforebreak>(\u003C\s\!)</beforebreak>
					<afterbreak/>
				</rule>

				<!-- Revendications|REVENDICATIONS -->
				<rule break="yes">
					<beforebreak>(Revendications|REVENDICATIONS)</beforebreak>
					<afterbreak/>
				</rule>

				<!-- Références -->
				<rule break="yes">
					<beforebreak>\s?(Références)\s?</beforebreak>
					<afterbreak/>
				</rule>

				<rule break="yes">
					<beforebreak/>
					<afterbreak>\s?(Références)\s?</afterbreak>
				</rule>

				<!-- Break #DESC# or #CLAIMS# #LANG# tag -->
				<rule break="yes">
					<beforebreak>\#(LANG|DESC_?([Ff][Rr]|[Dd][Ee])?|CLAIMS_?([Ff][Rr]|[Dd][Ee])?)\#\s?</beforebreak>
					<afterbreak/>
				</rule>

				<rule break="yes">
					<beforebreak/>
					<afterbreak>\#(LANG|DESC_?([Ff][Rr]|[Dd][Ee])?|CLAIMS_?([Ff][Rr]|[Dd][Ee])?)\#</afterbreak>
				</rule>

				<!-- Break p num tag -->
				<rule break="yes">
					<beforebreak>\u003Cp\snum\=("")\u003E</beforebreak>
					<afterbreak/>
				</rule>

				<!-- Break </claim-text> tag -->
				<rule break="yes">
					<beforebreak/>
					<afterbreak>\u003C\/claim-text\u003E</afterbreak>
				</rule>

				<!-- Break <claim-text> tag -->
				<rule break="yes">
					<beforebreak>\u003Cclaim-text\u003E</beforebreak>
					<afterbreak/>
				</rule>

				<!-- Break </?claims?> tag -->
				<rule break="yes">
					<beforebreak>\u003C\/?claims?\u003E</beforebreak>
					<afterbreak/>
				</rule>

				<!-- Break </fulltext-document> tag -->
				<rule break="yes">
					<beforebreak>\u003C\/?fulltext-document\u003E</beforebreak>
					<afterbreak/>
				</rule>

				<!-- Break claim id-lang-status tag -->
				<rule break="yes">
					<beforebreak/>
					<afterbreak>\u003Cclaims\sid\=\"(.*)\"\slang\=\"(.*)\"\sstatus\=\"(.*)\"\u003E</afterbreak>
				</rule>

				<!-- Break claim id-num tag -->
				<rule break="yes">
					<beforebreak>\u003Cclaim\sid\=\"(.*)\"\snum\=\"(.*)\"\u003E</beforebreak>
					<afterbreak/>
				</rule>

				<!-- Break p id num with blank tag -->
				<rule break="yes">
					<beforebreak>\u003Cp\sid\=("")\snum\=("")\u003E</beforebreak>
					<afterbreak/>
				</rule>

				<!-- Break p num id tag -->
				<rule break="yes">
					<beforebreak>\u003Cp\snum\=\"\d{4}\"\sid\=\"p\d{4}\"\u003E</beforebreak>
					<afterbreak/>
				</rule>

				<!-- Break p id num tag -->
				<rule break="yes">
					<beforebreak>\u003Cp\sid\=\"p\d{4}\"\snum\=\"\d{4}\"\u003E</beforebreak>
					<afterbreak/>
				</rule>

				<!-- Break <p> tag -->
				<rule break="yes">
					<beforebreak>\u003Cp\u003E</beforebreak>
					<afterbreak/>
				</rule>

				<!-- Break </p> tag -->
				<rule break="yes">
					<beforebreak/>
					<afterbreak>\u003C\/p\u003E</afterbreak>
				</rule>

				<!-- Break [0000] at the start of line -->
				<rule break="yes">
					<beforebreak>(?m)^\[[\d]{4,}\]</beforebreak>
					<afterbreak>\s</afterbreak>
				</rule>

			</languagerule>

			<!-- Text files format-related rule -->

			<languagerule languagerulename="Text">

				<!-- Special handling for Text files to break on empty indented lines idea by Jean-Christophe Helary -->
				<rule break="yes">
					<beforebreak>\n</beforebreak>
					<afterbreak> +</afterbreak>
				</rule>

			</languagerule>

			<!-- HTML format-related rule -->
			<languagerule languagerulename="HTML">

				<!-- Special handling for <br> tag to segment on it - idea by Jean-Christophe Helary -->
				<rule break="yes">
					<beforebreak>\s?&lt;br\d+\/?&gt;</beforebreak>
					<afterbreak>.</afterbreak>
				</rule>

			</languagerule>

		</languagerules>

		<maprules>
			<languagemap languagepattern=".*" languagerulename="GeneralImportant"/>
			<languagemap languagepattern="(DE|de).*" languagerulename="German"/>
			<languagemap languagepattern="(EN|en).*" languagerulename="English"/>
			<languagemap languagepattern="(FR|fr).*" languagerulename="French"/>
			<languagemap languagepattern=".*" languagerulename="Patent"/>
			<languagemap languagepattern=".*" languagerulename="Default"/>
			<languagemap languagepattern=".*" languagerulename="Text"/>
			<languagemap languagepattern=".*" languagerulename="HTML"/>
		</maprules>

	</body>
</srx>